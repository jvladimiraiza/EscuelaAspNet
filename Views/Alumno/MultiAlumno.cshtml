@* esto es un comentario *@
@model List<Alumno>

@{
    ViewData["Title"] = "Informacion Alumno";
    // recibiendo data que viene del controller 
    // @Model.nombre
}

<h1> @ViewData["Title"] </h1>
<table class="table table-dark table-hover">
    <thead>
        <tr>
            <th scope="col"> Id </th>
            <th scope="col"> Nombre </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var alumno in Model)
        {
            <tr>
                <td> @alumno.UniqueId </td>
                <td> @alumno.Nombre </td>
            </tr>
        }
    </tbody>
</table>
<p>@ViewData["Fecha"]</p>